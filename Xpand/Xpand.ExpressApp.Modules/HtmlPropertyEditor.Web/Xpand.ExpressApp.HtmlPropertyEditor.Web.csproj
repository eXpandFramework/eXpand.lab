  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
    <PackageReference Include="Xpand.XAF.Modules.ModelMapper" Version="1.1.34" />
    <PackageReference Include="DevExpress.ExpressApp.HtmlPropertyEditor.Web" Version="19.1.5" />
