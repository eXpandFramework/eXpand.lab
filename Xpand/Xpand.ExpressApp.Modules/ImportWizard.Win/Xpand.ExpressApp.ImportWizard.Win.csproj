<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyOriginatorKeyFile>..\..\Xpand.key\xpand.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <TargetFramework>net461</TargetFramework>
    <OutputPath>..\..\..\Xpand.DLL\</OutputPath>
    <LangVersion>latest</LangVersion>
    <DebugSymbols>true</DebugSymbols>
    <AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <DebugType>pdbonly</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="2.7.2" />
    <PackageReference Include="DevExpress.ExpressApp.Win" Version="19.1.5" />
    <PackageReference Include="DevExpress.Data" Version="19.1.5" />
    <PackageReference Include="DevExpress.ExpressApp.Xpo" Version="19.1.5" />
    <PackageReference Include="DevExpress.ExpressApp" Version="19.1.5" />
    <PackageReference Include="DevExpress.RichEdit.Export" Version="19.1.5" />
    <PackageReference Include="DevExpress.Office.Core" Version="19.1.5" />
    <PackageReference Include="DevExpress.RichEdit.Core" Version="19.1.5" />
    <PackageReference Include="DevExpress.Printing.Core" Version="19.1.5" />
    <PackageReference Include="DevExpress.Utils" Version="19.1.5" />
    <PackageReference Include="DevExpress.Xpo" Version="19.1.5" />
    <PackageReference Include="DevExpress.Win.Navigation" Version="19.1.5" />
    <PackageReference Include="DevExpress.Win.Printing" Version="19.1.5" />
    <PackageReference Include="DevExpress.Win.Grid" Version="19.1.5" />
    <PackageReference Include="DevExpress.Win" Version="19.1.5" />
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Design" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="WindowsBase" />
    <Reference Include="Xpand.ExpressApp.ImportWizard">
      <HintPath>..\..\..\Xpand.Dll\Xpand.ExpressApp.ImportWizard.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="Xpand.Persistent.Base">
      <HintPath>..\..\..\Xpand.Dll\Xpand.Persistent.Base.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
    <Reference Include="Xpand.Utils">
      <HintPath>..\..\..\Xpand.Dll\Xpand.Utils.dll</HintPath>
      <SpecificVersion>False</SpecificVersion>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Controllers\ImportWizViewController.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Controllers\ImportWizViewController.Designer.cs">
      <DependentUpon>ImportWizViewController.cs</DependentUpon>
    </Compile>
    <Compile Update="Forms\ProgressForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Forms\ProgressForm.designer.cs">
      <DependentUpon>ProgressForm.cs</DependentUpon>
    </Compile>
    <Compile Update="WinModule.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Properties\Resources.Designer.cs">
      <AutoGen>True</AutoGen>
      <DesignTime>True</DesignTime>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
    <Compile Update="Wizard\ExcelImportWizardProcessing.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Wizard\ResourceManagerSetter.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Wizard\ExcelImportWizard.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Wizard\ExcelImportWizard.designer.cs">
      <DependentUpon>ExcelImportWizard.cs</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Controllers\ImportWizViewController.resx">
      <DependentUpon>ImportWizViewController.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Forms\ProgressForm.resx">
      <DependentUpon>ProgressForm.cs</DependentUpon>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
      <SubType>Designer</SubType>
    </EmbeddedResource>
    <EmbeddedResource Update="Wizard\ExcelImportWizard.resx">
      <DependentUpon>ExcelImportWizard.cs</DependentUpon>
      <SubType>Designer</SubType>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Model.DesignedDiffs.xafml">
      <SubType>Designer</SubType>
    </EmbeddedResource>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Images\Import.png" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Images\Import.ico" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="Controllers\ListView\ViewController.cs" />
    <Compile Remove="Controllers\ListView\ViewController.Designer.cs" />
  </ItemGroup>
</Project>